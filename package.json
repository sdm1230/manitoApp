{
  "name": "manitoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint './**/*.{ts,tsx,js,jsx}'",
    "lint:fix": "eslint --fix './**/*.{ts,tsx,js,jsx}'",
    "prettier": "prettier --write --config .prettierrc.json './**/*.{ts,tsx}'",
    "build-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/ && rm -rf ./android/app/src/main/res/drawable-mdpi/ && rm -rf ./android/app/src/main/res/raw/",
    "release-android": "yarn build && cd ./android && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-seoul/kakao-login": "^2.10.2",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "axios": "^0.21.1",
    "jwt-decode": "^3.1.2",
    "mobx": "^6.1.6",
    "mobx-react": "^7.1.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-date-picker": "^3.2.9",
    "react-native-fast-image": "^8.3.4",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-image-picker": "^3.1.4",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.6.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-restart": "^0.0.20",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-swipe-gestures": "^1.0.5",
    "react-native-vector-icons": "^7.1.0",
    "socket.io-client": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/runtime": "7.12.5",
    "@bam.tech/react-native-make": "^3.0.0",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/expo": "^33.0.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.43",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/sass-loader": "^8.0.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "4.0.0",
    "husky": "^4.3.6",
    "jest": "25.5.4",
    "lint-staged": "^10.4.0",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "16.13.1",
    "sass-loader": "^10.0.3",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,html,css}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
