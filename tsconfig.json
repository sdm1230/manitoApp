{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "es6",
      "es2017"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "types": [
      "reflect-metadata",
      "jest"
    ],
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ]
  },
  "exclude": [
    "node_modules",
    "**/*.config.js",
    "**/*.spec.ts",
    "**/*.test.ts"
  ],
  "include": [
    "./**/*.tsx",
    "./**/*.ts"
, "src/common/atom/color.js", "src/common/organism/modal/guideModal/firstUser"  ]
}